class Tester {
	public static void main(String[] args) {
		// Implement your code here 
		int a=1;
		int b=4;
		int c=6;
		int discriminant=b*b-4*a*c;
		int x1=(-b+discriminant)/2*a;
		int x2=(-b-discriminant)/2*a;
		if(discriminant==0){
		    System.out.println("The Value of root of Equations is "+ x1);
		}else if(discriminant > 0){
		    System.out.println("The Value of root of Equations are  "+ x1+ "and "+ x2);
		}else{
		    System.out.println("The Equation has no Equal Roots");
		}
	}
}

