class Tester {
	public static void main(String[] args) {
		// Implement your code here 
		
		int Qualityordered=2;
		int Distance=3;
	    String FoodType="N";
		int delivary;
	if(FoodType=="N"){	
		if(Distance>=3){
		    delivary=0;
		}else if(Distance>3 && Distance<=6){
		    delivary=Distance*1;
		}else{
		    delivary=Distance*2;
		}
			int totalprice=15*Qualityordered+delivary;
			System.out.println(totalprice);
	}else if(FoodType=="V"){
	    if(Distance>=3){
		    delivary=0;
		}else if(Distance>3 && Distance<=6){
		    delivary=Distance*1;
		}else{
		    delivary=Distance*2;
		}
			int totalprice=12*Qualityordered+delivary;
			System.out.println(totalprice);
	}
	else{
	    System.out.println(-1);
	}
}}
